/**app.wxss**/
page {
  --themeColor: #f2964f;
}

.container1 {  
  height: 100%;  
  display: flex;  
  flex-direction: column;  
  align-items: center;  
  justify-content: space-between;  
  padding: 200rpx 0;  
  box-sizing: border-box;  
}

.arrow {
  width: 8px;
  height: 8px;
  border-top: 3px solid #999;
  border-right: 3px solid #999;
  transform: rotate(45deg);
}